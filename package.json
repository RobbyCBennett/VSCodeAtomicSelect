{	
	"name": "atomic-select",
	"displayName": "Atomic Select",
	"description": "Navigate and select words, quotes, and brackets.",
	"version": "1.0.0",
	"publisher": "rob-bennett",
	"repository": {
		"type": "git",
		"url": "https://github.com/RobbyCBennett/VSCodeAtomicSelect.git"
	},
	"engines": {
		"vscode": "^1.54.0"
	},
	"categories": [
		"Keymaps",
		"Other"
	],
	"keywords": [
		"Select",
		"Word",
		"Navigate",
		"Quote",
		"Bracket"
	],
	"icon": "icon.png",
	"activationEvents": [
        "onCommand:atomicSelect.selectWord",
        "onCommand:atomicSelect.selectAllWords",
        "onCommand:atomicSelect.moveToStartOfWord",
        "onCommand:atomicSelect.selectToStartOfWord",
        "onCommand:atomicSelect.moveToEndOfWord",
        "onCommand:atomicSelect.selectToEndOfWord"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "atomicSelect.selectWord",
				"title": "Atomic Select: Select Word",
				"when": "editorTextFocus"
			},
			{
				"command": "atomicSelect.selectAllWords",
				"title": "Atomic Select: Select All Words",
				"when": "editorTextFocus"
			},
			{
				"command": "atomicSelect.moveToStartOfWord",
				"title": "Atomic Select: Move to Start of Word",
				"when": "editorTextFocus"
			},
			{
				"command": "atomicSelect.selectToStartOfWord",
				"title": "Atomic Select: Select to Start of Word",
				"when": "editorTextFocus"
			},
			{
				"command": "atomicSelect.moveToEndOfWord",
				"title": "Atomic Select: Move to End of Word",
				"when": "editorTextFocus"
			},
			{
				"command": "atomicSelect.selectToEndOfWord",
				"title": "Atomic Select: Select to End of Word",
				"when": "editorTextFocus"
			}
		]
	}
}
